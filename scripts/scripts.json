{
  "__scripts": {
    "build:analyze:bigmem": "cross-env ANALYZE=true node --max-old-space-size=12288 ./node_modules/next/dist/bin/next build --webpack",
    "i18n:check": "node scripts/check-hardcoded.js",
    "i18n:sync": "node scripts/sync-locales.js",
    "check-updates": "npx npm-check-updates",
    "build:analyze": "cross-env ANALYZE=true next build",
    "update-libs": "npx npm-check-updates -u && npm install",
    "copyright:add": "node scripts/add-copyright.js --dry-run",
    "generate:thumbs": "node scripts/generate-thumbs.js",
    "generate:locales-list": "node scripts/generate-locales-list.js",
    "create:international-locales": "node scripts/create-international-locales.js",
    "generate:page-from-locale": "node scripts/generate-page.js",
    "meta:add": "node scripts/meta-tools.js add",
    "meta:refine": "node scripts/meta-tools.js refine",
    "meta:all": "node scripts/meta-tools.js all",
    "complete-clean": "Remove-Item node_modules, package-lock.json, dist, build, .next, .cache, coverage, .turbo, .vite, .parcel-cache -Recurse -Force -ErrorAction SilentlyContinue",
    "clean": "Remove-Item node_modules, package-lock.json, .next, .cache, .parcel-cache -Recurse -Force -ErrorAction SilentlyContinue",
    "generate:component-from-locale": "node scripts/generate-component.js",
    "generate:analytics": "node scripts/generate-analytics-json.js",
    "noEmit:bigmem": "node --max-old-space-size=8192 ./node_modules/typescript/bin/tsc --noEmit",
    "generate-meta-keywords": "node scripts/generate-meta-keywords.js"
  }
}